{
  "name": "virtual-venue-client",
  "version": "1.0.0",
  "description": "Repo for Virtual Venue",
  "scripts": {
    "build": "parcel build src/index.html src/screens/equipment-room/index.html src/screens/winter-show/index.html src/winter-show/index.html src/equipment-room/index.html --public-url /",
    "watch": "export ENVIRONMENT=dev && parcel src/index.html src/screens/equipment-room/index.html src/screens/winter-show/index.html src/winter-show/index.html src/equipment-room/index.html",
    "start-server": "export ENVIRONMENT=dev && node server/index.js",
    "production": "npm run build && forever start server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AidanNelson/virtual-venue.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AidanNelson/virtual-venue/issues"
  },
  "homepage": "https://github.com/AidanNelson/virtual-venue#readme",
  "dependencies": {
    "@fullcalendar/core": "^5.11.3",
    "@fullcalendar/daygrid": "^5.11.3",
    "@fullcalendar/google-calendar": "^5.11.3",
    "@fullcalendar/list": "^5.11.3",
    "@fullcalendar/timegrid": "^5.11.3",
    "lightningcss": "^1.16.0",
    "mediasoup-client": "^3.6.51",
    "request": "^2.88.2",
    "simple-mediasoup-peer-client": "^0.0.3",
    "socket.io-client": "^4.4.1",
    "three": "^0.139.2"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "parcel": "^2.7.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "util": "^0.12.5"
  }
}
